generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
}

model Meal {
  id       Int     @id @default(autoincrement())
  title    String  @db.VarChar(255)
  add_ons  String @default("ONLY")
  price    Float
  category String
}
